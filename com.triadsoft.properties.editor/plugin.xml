<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.triadsoft.properties.editors.PropertiesEditor"
            contributorClass="com.triadsoft.properties.editors.MultiPageEditorContributor"
            extensions="properties"
            icon="icons/page_white_world.png"
            id="com.triadsoft.properties.editors.PropertiesEditor"
            name="%localizedProperties.editor.name">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.triadsoft.properties.preferences.LocalizedPropertiesPreferencePage"
            id="com.triadsoft.properties.preferences.LocalizedPropertiesPreferencePage"
            name="%localizedProperties.preferences.name">
      </page>
      <page
            class="com.triadsoft.properties.preferences1.SamplePreferencePage"
            id="com.triadsoft.properties.preferences1.SamplePreferencePage"
            name="Sample Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.triadsoft.properties.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="com.triadsoft.properties"
            name="Localized Properties">
      </category>
      <wizard
            category="com.triadsoft.properties"
            class="com.triadsoft.properties.wizards.LocalizedProperties"
            icon="icons/page_white_world.png"
            id="com.triadsoft.properties.wizards.LocalizedProperties"
            name="Localized Properties">
      </wizard>
   </extension>

</plugin>
